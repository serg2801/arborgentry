<tr>
  <td><%= vendor_form.business_name %></td>
  <td><%= vendor_form.first_name %></td>
  <td><%= vendor_form.last_name %></td>
  <td><%= vendor_form.email %></td>
  <td><%= vendor_form.phone_number %></td>
  <% if vendor_form.grant_access %>
      <td><%= link_to 'Vendor was created', '#' %></td>
  <% else %>
      <td><%= link_to 'Create Vendor', grant_access_path(vendor_form) %></td>
  <% end %>
  <% if vendor_form.vendor_form_agreements.blank? %>
      <td>Vendor Agreement not uploaded.</td>
  <% else %>
      <td>Vendor Agreement uploaded.</td>
  <% end %>
  <td><%= vendor_form.created_at.strftime('%H:%M | %d %b %Y') %></td>
  <td><%= link_to 'Show', vendor_form_path(vendor_form), class: 'btn btn-info btn-right btn-xs' %></td>
</tr>
