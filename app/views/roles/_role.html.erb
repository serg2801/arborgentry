<tr>
  <td><%= role.name %></td>
  <td><%= role.created_at.strftime('%H:%M | %d %b %Y') %></td>
  <td>
    <%= link_to "Show", role_path(role) %>
    <% if current_vendor == role.vendor_creator %>
			 /
			<%= link_to "Delete", role_path(role), method: :delete, data: {confirm: "You sure?"}, remote: true %>
	<% end %>
  </td>
</tr>
