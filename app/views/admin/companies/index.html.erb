<div id="page-header" class="clearfix">
</div>
<!-- Start .row -->
<div class="row">
  <div class="col-lg-12">
    <!-- col-lg-12 start here -->
    <div class="panel panel-default toggle panelMove panelClose panelRefresh">
      <!-- Start .panel -->
      <div class="panel-heading">
        <h4 class="panel-title" style="float:left">Company List</h4>
        <h4>
          (<%= link_to "Add New", new_admin_company_path, remote: true %>)</h4>
      </div>
      <div class="panel-body">
        <table id="responsive-datatables" class="table table-bordered table-striped table-hover dt-responsive non-responsive" cellspacing="0" width="100%">
          <thead>
          <tr>
            <th>Company Name</th>
            <th>Edit / Delete</th>
          </tr>
          </thead>
          <tbody id="company-list">
          <%= render @companies %>
          </tbody>
        </table>
      </div>
    </div>
    <!-- End .panel -->
  </div>
  <!-- col-lg-12 end here -->
</div>
<!-- End .row -->
<!-- Back to top -->
<div id="back-to-top"><a href="#">Back to Top</a>
</div>

